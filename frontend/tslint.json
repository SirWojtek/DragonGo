{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "no-empty": false,
    "array-type": [true, "array"],
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": [
      "./src/app/utils/RecursivePartial.ts"
    ]
  }
}
